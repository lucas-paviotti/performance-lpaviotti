{
  "name": "server-paviottilucas",
  "version": "1.0.0",
  "description": "Servidor Express de Lucas Paviotti",
  "main": "server.es6.js",
  "scripts": {
    "start": "nodemon --ignore db/ --ignore node_modules/ server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "forever": "forever start server.js && forever list",
    "forever-watch": "forever start -w server.js && forever list",
    "pm2": "pm2 start server.js",
    "pm2-fork": "pm2 start server.js --name server-fork",
    "pm2-cluster": "pm2 start server.js -i max -f",
    "pm2-fork-watch": "pm2 start ecosystem.config.js",
    "pm2-cluster-watch": "pm2 start ecosystem-cluster.config.js"
  },
  "author": "Lucas Paviotti",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "cluster": "^0.7.7",
    "compression": "^1.7.4",
    "connect-mongo": "^4.6.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.3",
    "express-session": "^1.17.2",
    "faker": "^5.5.3",
    "log4js": "^6.3.0",
    "mongoose": "^6.0.11",
    "mongoose-findorcreate": "^3.0.0",
    "nodemon": "^2.0.13",
    "normalizr": "^3.6.1",
    "passport": "^0.5.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "socket.io": "^4.2.0",
    "uuid": "^8.3.2"
  }
}
